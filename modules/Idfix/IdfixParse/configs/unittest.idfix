                -icon=time
                -cols=2
            #option
                -title=Multiple options possible
                -icon=tags
                -cols=6
            #number
                -title=Numeric values
                -icon=phone
                -cols=4
            #internet
                -title=Internet related
                -icon=globe
                -cols=3
            #form
                -title=Internal Form Elements
                -icon=list-alt
                -cols=4
            #text
                -title=Text related
                -icon=align-left
                -cols=3
        #fields
            #button
                -type=button
                -value=Click me
                -group=form
            #text
                -type=text
