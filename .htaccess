Options +FollowSymLinks
Options -Multiviews

<IfModule mod_rewrite.c>



# Turn rewriting ON
RewriteEngine On

# Special for OnePP
# If no path is specified we serve the cached html
# file from the production directory
RewriteCond %{REQUEST_URI} !/(.+)
RewriteRule ^(.*)$ /files/onepp/dev/%{HTTP_HOST}.html [L] 

#RewriteCond %{REQUEST_FILENAME} !-f
#RewriteCond %{REQUEST_FILENAME} !-d
#RewriteRule ^(.*)$ index.php/$1?%{QUERY_STRING} [L]

#RewriteRule idfix/(.*)$ index.php?idfix=$1 [QSA,NC,L]
#RewriteRule event/(.*)$ index.php?event=$1 [QSA,NC,L]
#RewriteRule (.*)$ index.php [QSA,NC,L]
</IfModule>